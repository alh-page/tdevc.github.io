                                <th scope="col">Target</th>
                                <th scope="col">Conversion</th>
                            </tr>
                        </thead>
                        <tbody>
                            {% for src_basename, tgt_basename, src, tgt, YourTTS, FreeVC, DDDMVC, Ours in s2s_rows%}
